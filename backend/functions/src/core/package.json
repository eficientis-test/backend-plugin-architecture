	},
	"dependencies": {
		"firebase-admin": "^13.0.2",
		"fireorm": "^0.23.3",
		"express": "^4.17.1",
		"cors": "^2.8.5",
		"apollo-server-core": "^3.13.0",
		"apollo-server-express": "^3.13.0",
		"graphql": "^15.5.0",
		"type-graphql": "^1.1.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"zod": "^3.23.8"
	}
}
